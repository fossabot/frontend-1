  "author": "Roman @Realetive Ganin",
  "name": "nevatrip-site",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/Nevatrip/frontend.git"
  },
  "engines": {
    "node": ">=6"
  },
  "browserslist": [
    "ie >= 10",
    "opera 12.1",
    "> 2%",
    "last 2 versions"
  ],
  "dependencies": {
    "bem-components": "^6.0.1",
    "bem-core": "^3.2.1",
    "bem-forms": "github:bem-contrib/bem-forms#v2.0.0"
  },
  "devDependencies": {
    "babel-preset-env": "^1.6.1",
    "bem-lib-site": "github:Realetive/bem-lib-site#master",
    "bem-tools-core": "^0.0.3",
    "bem-tools-create": "^2.2.0",
    "bemhint": "^0.10.0",
    "bemhint-css-naming": "^1.0.1",
    "bemhint-deps-specification": "^1.0.0",
    "bemhint-fs-naming": "^1.0.0",
    "body-parser": "^1.18.2",
    "chokidar": "^1.7.0",
    "chokidar-cli": "^1.2.0",
    "cldr-data": "^32.0.0",
    "cldrjs": "^0.5.0",
    "compression": "^1.7.1",
    "connect-slashes": "^1.3.1",
    "cookie-parser": "^1.4.3",
    "cookie-session": "^2.0.0-beta.3",
    "csurf": "^1.9.0",
    "debug-http": "^1.1.0",
    "dotenv": "^4.0.0",
    "enb": "^1.5.1",
    "enb-babelify": "^0.0.1",
    "enb-bem-i18n": "^1.1.1",
    "enb-bem-techs": "^2.2.2",
    "enb-bemxjst": "^8.6.7",
    "enb-bemxjst-i18n": "github:tadatuta/enb-bemxjst-i18n#bemtree-nextgen",
    "enb-borschik": "^3.0.0",
    "enb-js": "^1.1.1",
    "enb-modules": "github:Realetive/enb-modules",
    "enb-postcss": "^2.0.0",
    "errorhandler": "^1.5.0",
    "eslint": "^4.11.0",
    "eslint-config-pedant": "^1.0.0",
    "eslint-plugin-bem-xjst": "^2.2.0",
    "express": "^4.16.2",
    "express-pino-logger": "^3.0.1",
    "express-session": "^1.15.6",
    "lodash": "^4.17.4",
    "lost": "^8.2.0",
    "markdown-bemjson": "^3.1.1",
    "mkdirp": "^0.5.1",
    "mobile-detect": "^1.4.1",
    "node-eval": "^2.0.0",
    "node-notifier": "^5.1.2",
    "node-notifier-cli": "^1.0.1",
    "nodemon": "^1.12.1",
    "normalizr": "^3.2.4",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "pobem": "^1.1.2",
    "postcss": "^6.0.14",
    "postcss-cssnext": "^3.0.2",
    "postcss-each": "^0.10.0",
    "postcss-for": "^2.1.1",
    "postcss-import": "^11.0.0",
    "postcss-inline-svg": "^3.0.0",
    "postcss-nested": "^2.1.2",
    "postcss-reporter": "^5.0.0",
    "postcss-simple-vars": "^4.1.0",
    "postcss-url": "^7.1.2",
    "pre-commit": "^1.2.2",
    "pretty-error": "^2.1.1",
    "serve-favicon": "^2.4.5",
    "serve-static": "^1.13.1",
    "stylelint": "^8.2.0",
    "stylelint-config-standard": "^17.0.0",
    "tiny-lr": "^1.0.5",
    "universal-router": "^5.0.0",
    "ym": "^0.1.2"
  },
  "scripts": {
    "start": "node -r dotenv/config server",
    "postinstall": "npm run drop-cache && npm run make",
    "drop-cache": "rm -rf .enb/tmp",
    "make": "node -r dotenv/config node_modules/enb/bin/enb make",
    "dev": "npm run drop-cache && npm run make && nodemon",
    "prod": "npm run drop-cache && npm run make",
    "lint": "eslint . && bemhint .",
    "stylelint": "stylelint static/assets/css/**/*.css",
    "test": "npm run lint",
    "watch": "chokidar 'components/**' --initial -c 'enb make && notify -t $HOST -m Built'"
  },
  "pre-commit": [
    "test"
  ],
  "nodemonConfig": {
    "restartable": "rs",
    "ignore": [
      ".git",
      "node_modules/**/node_modules"
    ],
    "verbose": false,
    "execMap": {
      "js": "node"
    },
    "watch": [
      "server",
      ".enb"
    ],
    "ext": "js"
  },
  "private": true
}
